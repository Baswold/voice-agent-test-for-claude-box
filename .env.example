# LiveKit Configuration
LIVEKIT_URL=wss://your-livekit-url.livekit.cloud
LIVEKIT_API_KEY=your-livekit-api-key
LIVEKIT_API_SECRET=your-livekit-api-secret

# LLM Provider (OpenAI or Anthropic)
OPENAI_API_KEY=your-openai-api-key
ANTHROPIC_API_KEY=your-anthropic-api-key

# STT Provider (Deepgram recommended)
DEEPGRAM_API_KEY=your-deepgram-api-key

# TTS Provider (Cartesia or ElevenLabs)
CARTESIA_API_KEY=your-cartesia-api-key
ELEVENLABS_API_KEY=your-elevenlabs-api-key

# Agent Configuration
DEFAULT_LLM=openai/gpt-4o-mini
DEFAULT_STT=local/whisper-base
DEFAULT_TTS=local/macos

# CLI Settings
ROOM_NAME=voice-agent-test
AGENT_MOOD=friendly
SESSION_TIMEOUT=300

# Demo Features (set to "true" to enable)
VERBOSE_TOOL_LOGGING=false
AUTO_GREETING=false
SIMULATED_LATENCY=false
LATENCY_MS=1000
SOUND_EFFECTS=false
TOOL_TIMELINE=false
TRANSCRIPT_MODE=false
DEBUG_MODE=false
ANNOUNCEMENT_MODE=false
CONVERSATION_SUMMARY=true
MOCK_MODE=false
